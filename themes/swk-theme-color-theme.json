// * `swekiiz` : this is my first vscode theme
{
	"name": "swk-theme",
	"type": "dark",
	"author": "swekiiz",
	"semanticHighlighting": false,
	"semanticTokenColors": {
		"parameter:cpp": {
			"foreground": "#cfcfcf"
		},
		"namespace:cpp": {
			"foreground": "#d1c7bd"
		},
		"property": {
			"foreground": "#d5f34f"
		},
		// "enumMember": {
		// 	"foreground": "#56b6c2"
		// },
		// "variable.constant": {
		// 	"foreground": "#d19a66"
		// },
		// "variable.defaultLibrary": {
		// 	"foreground": "#e5c07b"
		// },
	},
	"tokenColors": [
		/** --------------------------------------------------------------------------------
		 *  * Source
		 *  --------------------------------------------------------------------------------
		 */
		{
			"name": "Comments",
			"scope": "source.dart",
			"settings": {
				"foreground": "#e7e7e7"
			}
		},
		{
			"name": "java source",
			"scope": "source.java",
			"settings": {
				"foreground": "#e7e7e7"
			}
		},
		/** --------------------------------------------------------------------------------
		 *  * Comments
		 *  --------------------------------------------------------------------------------
		 */
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Block Comment",
			"scope": [
				"comment storage.type",
				"comment punctuation.definition.block"
			],
			"settings": {
				"foreground": "#6D6D6D",
			}
		},
		{
			"name": "ts: triple slash comment",
			"scope": "source.ts comment.line.triple-slash.directive.ts punctuation.definition.comment.ts",
			"settings": {
				"foreground": "#6164ff",
			}
		},
		{
			"name": "ts: triple slash tag",
			"scope": "source.ts comment.line.triple-slash.directive.ts meta.tag.ts punctuation.definition.tag.directive.ts",
			"settings": {
				"foreground": "#61abff",
			}
		},
		// {
		// 	"name": "Comment Markup Link",
		// 	"scope": "comment markup.link",
		// 	"settings": {
		// 		"foreground": "#888888"
		// 	}
		// },
		/** --------------------------------------------------------------------------------
		 *  * String
		 *  --------------------------------------------------------------------------------
		 */
		{
			"name": "Default meta block",
			"scope": "meta",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "String single quoted",
			"scope": "string.quoted.single",
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "String double quoted",
			"scope": "meta.var.expr string.quoted.double",
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "js: import string",
			"scope": [
				"source.js string.quoted.module.js"
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "js: string double quoted",
			"scope": "string.quoted.double.js",
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "ts: string double quoted",
			"scope": "string.quoted.double.ts",
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "dart: string double quoted",
			"scope": [
				"source.dart string.interpolated.double.dart"
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "java: string double quoted",
			"scope": [
				"string.quoted.double.java"
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "dart: import string",
			"scope": [
				"string.interpolated.single.dart"
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "c++: string double quoted",
			"scope": [
				"string.quoted.double.cpp",
				"string.quoted.double.c"
			],
			"settings": {
				"foreground": "#e0ae24"
			}
		},
		{
			"name": "String unquoted",
			"scope": "string.unquoted",
			"settings": {
				"foreground": "#f7c376"
			}
		},
		{
			"name": "String single quoted punctuation",
			"scope": "string.quoted.single punctuation.definition.string.begin,string.quoted.single punctuation.definition.string.end",
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "String in tag except HTML",
			"scope": "meta.tag string.quoted.double",
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "HTML Punctuation, String",
			"scope": "string.quoted.double.html, punctuation.definition.string.begin.html, punctuation.definition.string.end.html",
			"settings": {
				"foreground": "#e09be2"
			}
		},
		{
			"name": "vue: import string",
			"scope": [
				"source.vue source.js string.quoted.module.js"
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "Template String (``my template string``)",
			"scope": [
				"string.template",
				"punctuation.definition.string.template"
			],
			"settings": {
				"foreground": "#f7c376"
			}
		},
		{
			"name": "js: Template String (``my template string``)",
			"scope": [
				"string.quoted.template.js",
				"punctuation.definition.string.template",
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "react: typescript import string",
			"scope": [
				"meta.import.tsx string.quoted.single.tsx",
				"meta.import.tsx string.quoted.double.tsx"
			],
			"settings": {
				"foreground": "#69f0ae"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "php: Single Quote HMTL Fix",
			"scope": "punctuation.definition.string.end.php, punctuation.definition.string.begin.php",
			"settings": {
				"foreground": "#9AA83A"
			}
		},
		{
			"name": "env: key",
			"scope": [
				"variable.other.env"
			],
			"settings": {
				"foreground": "#f7bf76"
			}
		},
		// {
		// 	"name": "HTML Tag Value",
		// 	"scope": "meta.tag string -source -punctuation, text source text meta.tag string -punctuation",
		// 	"settings": {
		// 		"foreground": "#6089B4"
		// 	}
		// },
		{
			"name": "Tag Attribute",
			"scope": "meta.tag punctuation.definition.string ",
			"settings": {
				"foreground": "#d0b340"
			}
		},
		// {
		// 	"name": "python placeholder reset to normal string",
		// 	"scope": "constant.character.format.placeholder.other.python",
		// 	"settings": {
		// 		"foreground": "#8080FF"
		// 	}
		// },
		/** --------------------------------------------------------------------------------
		 *  * Regular Expressions
		 *  --------------------------------------------------------------------------------
		 */
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp",
				"string.regexp keyword.control",
				"string.regexp keyword.operator"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Regex Container Punctuation ( / )",
			"scope": [
				"string.regexp punctuation.definition.string"
			],
			"settings": {
				"foreground": "#A9B9B0"
			}
		},
		{
			"name": "Regex Mode (`igm`)",
			"scope": [
				"string.regexp keyword.other"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		/** --------------------------------------------------------------------------------
		 *  * Keywords
		 *  --------------------------------------------------------------------------------
		 */
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#9575cd"
			}
		},
		{
			"name": "dart: Keywords",
			"scope": "keyword.declaration.dart",
			"settings": {
				"foreground": "#bb7c29"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "Import module JS",
