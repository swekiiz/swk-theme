				"string.regexp keyword.operator"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Regex Container Punctuation ( / )",
			"scope": [
				"string.regexp punctuation.definition.string"
			],
			"settings": {
				"foreground": "#A9B9B0"
			}
		},
		{
			"name": "Regex Mode (`igm`)",
			"scope": [
				"string.regexp keyword.other"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
