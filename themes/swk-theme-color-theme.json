				"keyword.operator.module"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "react: (jsx, tsx) default keyword",
			"scope": [
				"keyword.control.default.js",
				"keyword.control.default.jsx",
				"keyword.control.default.ts",
				"keyword.control.default.tsx"
			],
			"settings": {
				"foreground": "#f7d064"
			}
		},
		{
			"name": "dart: operator.accessor ( . )",
			"scope": [
				"punctuation.dot.dart"
			],
			"settings": {
				"foreground": "#ff379b"
			}
		},
		{
			"name": "js: operator.arithmetic ( +, -, *, / )",
			"scope": [
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "js: operator.accessor ( . )",
			"scope": [
				"keyword.operator.accessor"
			],
			"settings": {
				"foreground": "#ff379b"
			}
		},
		{
			"name": "js: operator.assignment ( = )",
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "js: comma ( , )",
			"scope": [
				"meta.delimiter.comma.js"
			],
			"settings": {
				"foreground": "#fffde8"
			}
		},
		{
			"name": "dart: comma ( , )",
			"scope": [
				"source.dart punctuation.comma.dart"
			],
			"settings": {
				"foreground": "#a0ff88"
			}
		},
		{
			"name": "js, ts: import keyword",
			"scope": [
				"keyword.operator.expression.import",
			],
			"settings": {
				"foreground": "#FFD14A"
			}
		},
		{
			"name": "operator logical (&&, ||)",
			"scope": "keyword.operator.logical",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "operator bitwise (<<, >>, &, |, ^)",
			"scope": "keyword.operator.bitwise",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "arrow function operator ( => )",
			"scope": "storage.type.function.arrow",
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "spread operator ( ... )",
			"scope": "keyword.operator.spread",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "operator channel",
			"scope": "keyword.operator.channel",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "TS type operator",
			"scope": "keyword.operator.type",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "js, ts, jsx, tsx: (`=`) assignment operator",
			"scope": "meta.tag keyword.operator.assignment",
			"settings": {
				"foreground": "#d64f4f"
			}
		},
		{
			"name": "Accessor Operator (`.`)",
			"scope": [
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#ff379b"
			}
		},
		{
			"name": "ts, tsx: semicolon (`;`)",
			"scope": [
				"punctuation.terminator.statement.ts",
				"punctuation.terminator.statement.tsx"
			],
			"settings": {
				"foreground": "#ffdddd"
			}
		},
		{
			"name": "dart: semicolon (`;`)",
			"scope": [
				"source.dart meta.declaration.dart punctuation.terminator.dart",
				"source.dart punctuation.terminator.dart"
			],
			"settings": {
				"foreground": "#ffdddd"
			}
		},
		{
			"name": "python: (`.`) operator",
			"scope": [
				"punctuation.separator.period.python"
			],
			"settings": {
				"foreground": "#ff00bf"
			}
		},
		{
			"name": "js/ts Keyword",
